{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/.auth/*",
      "/assets/*",
      "/*.css",
      "/*.js",
      "/*.map",
      "/*.ico",
      "/*.png",
      "/*.svg"
    ]
  },
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ]
}
