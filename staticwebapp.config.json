{
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["anonymous"],
      "redirect": "/.auth/login/aad"
    },
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/.auth/*",
      "/assets/*",
      "/*.css",
      "/*.js",
      "/*.map",
      "/*.ico",
      "/*.png",
      "/*.svg"
    ]
  }
}
